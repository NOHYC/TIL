   "source": [
    "class Solution:\n",
    "    def removePalindromeSub(self, s: str) -> int:\n",
    "        s_len = len(s)\n",
    "        if s_len == 0:\n",
    "            return 0\n",
    "        for i in range(s_len//2):\n",
    "             if s[i] != s[-(i+1)]:\n",
    "                return 2\n",
    "        return 1"
   ]
  }
